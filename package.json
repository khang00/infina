{
  "name": "infina",
  "version": "1.0.0",
  "description": "Order like saving managaement",
  "main": "src/index.js",
  "repository": "git@github.com:khang00/infina.git",
  "author": "dlqkhang8@gmail.com",
  "license": "MIT",
  "scripts": {
    "postbuild": "yarn run copyGraphQL",
    "copyGraphQL": "copyfiles --error --up 1 ./src/graphql/*.* dist",
    "clean": "rimraf dist/*",
    "lint": "prettier --write .",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs,graphql --exec yarn run dev:start",
    "start": "node dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-tools/graphql-file-loader": "7.3.14",
    "@graphql-tools/load": "7.5.13",
    "apollo-server-express": "3.8.2",
    "express": "^4.18.1",
    "express-graphql": "0.12.0",
    "graphql": "16.5.0",
    "mongoose": "6.3.5"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.0",
    "@types/express": "4.17.13",
    "@types/node": "^17.0.39",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.16",
    "prettier": "2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "yarn-run-all": "^3.1.1"
  }
}
